<div class="container my-5">
    <div class="cards">
      <% @cars.each do |car| %>
        <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_tag car.photo.key, height: 300, width: 400, crop: :fill %>')">
          <%= link_to car.title, car_path(car.id), class: "link-to-text-color" %>
        </div>
      <% end %>
    </div>
</div>
